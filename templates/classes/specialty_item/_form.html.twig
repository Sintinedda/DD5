{{ form_start(form) }}
    {{ form_errors(form) }}
    <div>
        {{ form_row(form.part1) }}
        {{ form_row(form.part2) }}
        {{ form_row(form.name) }}
        {{ form_row(form.slug) }}
    </div>
    <div>
        {{ form_row(form.source) }}
        {{ form_row(form.source_part) }}
    </div>
    <div>
        {{ form_row(form.d1) }}
    </div>
    <div>
        {{ form_row(form.d2) }}
    </div>
    <div>
        {{ form_row(form.d3) }}
    </div>
    <div>
        {{ form_row(form.d4) }}
    </div>
    <div>
        {{ form_row(form.d5) }}
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
