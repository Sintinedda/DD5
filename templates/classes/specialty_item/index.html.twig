{% extends 'base.html.twig' %}

{% block title %}SpecialtyItem index{% endblock %}

{% block body %}
    <h1>SpecialtyItem index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Part1</th>
                <th>Part2</th>
                <th>Name</th>
                <th>Slug</th>
                <th>D1</th>
                <th>D2</th>
                <th>D3</th>
                <th>D4</th>
                <th>D5</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for specialty_item in specialty_items %}
            <tr>
                <td>{{ specialty_item.id }}</td>
                <td>{{ specialty_item.part1 }}</td>
                <td>{{ specialty_item.part2 }}</td>
                <td>{{ specialty_item.name }}</td>
                <td>{{ specialty_item.slug }}</td>
                <td>{{ specialty_item.d1 }}</td>
                <td>{{ specialty_item.d2 }}</td>
                <td>{{ specialty_item.d3 }}</td>
                <td>{{ specialty_item.d4 }}</td>
                <td>{{ specialty_item.d5 }}</td>
                <td>
                    <a href="{{ path('app_classes_specialty_item_show', {'id': specialty_item.id}) }}">show</a>
                    <a href="{{ path('app_classes_specialty_item_edit', {'id': specialty_item.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="11">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_classes_specialty_item_new') }}">Create new</a>
{% endblock %}
