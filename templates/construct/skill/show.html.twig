{% extends 'base.html.twig' %}

{% block title %}Skill{% endblock %}

{% block body %}
    <h1>Skill</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ skill.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ skill.name }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ skill.level }}</td>
            </tr>
            <tr>
                <th>Show_skill</th>
                <td>{{ skill.showSkill ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Optional</th>
                <td>{{ skill.optional ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>D1</th>
                <td>{{ skill.d1 }}</td>
            </tr>
            <tr>
                <th>D2</th>
                <td>{{ skill.d2 }}</td>
            </tr>
            <tr>
                <th>D3</th>
                <td>{{ skill.d3 }}</td>
            </tr>
            <tr>
                <th>D4</th>
                <td>{{ skill.d4 }}</td>
            </tr>
            <tr>
                <th>D5</th>
                <td>{{ skill.d5 }}</td>
            </tr>
            <tr>
                <th>D6</th>
                <td>{{ skill.d6 }}</td>
            </tr>
            <tr>
                <th>D7</th>
                <td>{{ skill.d7 }}</td>
            </tr>
            <tr>
                <th>D8</th>
                <td>{{ skill.d8 }}</td>
            </tr>
            <tr>
                <th>D9</th>
                <td>{{ skill.d9 }}</td>
            </tr>
            <tr>
                <th>D10</th>
                <td>{{ skill.d10 }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ skill.type }}</td>
            </tr>
            <tr>
                <th>Part</th>
                <td>{{ skill.part }}</td>
            </tr>
            <tr>
                <th>Attack_type</th>
                <td>{{ skill.attackType }}</td>
            </tr>
            <tr>
                <th>Damage</th>
                <td>{{ skill.damage }}</td>
            </tr>
            <tr>
                <th>Target</th>
                <td>{{ skill.target }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_construct_skill_index') }}">back to list</a>

    <a href="{{ path('app_construct_skill_edit', {'id': skill.id}) }}">edit</a>

    {{ include('construct/skill/_delete_form.html.twig') }}
{% endblock %}
