<div class="accordion-item">
    <h2 class="accordion-header">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#sense" aria-expanded="true" aria-controls="collapseOne">
            Sens  
        </button>
    </h2>
    <a href="{{ path('sense_new') }}">Create new</a>
    <div id="sense" class="accordion-collapse collapse" data-bs-parent="#divers">
        <div class="accordion-body">
            <table class="table">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Abbreviation</th>
                        <th>actions</th>
                    </tr>
                </thead>
                <tbody>
                    {% for sense in senses %}
                        <tr>
                            <td>{{ sense.id }}</td>
                            <td><a href="{{ path('sense_edit', {'id': sense.id}) }}">{{ sense.abbreviation }}</a></td>
                            <td>{{ include('assets/sense/_delete_form.html.twig') }}</td>
                        </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>  
    </div>
</div>