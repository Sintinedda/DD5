{% extends 'base.html.twig' %}

{% block title %}BGSkill index{% endblock %}

{% block body %}
    <h1>BGSkill index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>D1</th>
                <th>D2</th>
                <th>D3</th>
                <th>D4</th>
                <th>D5</th>
                <th>D6</th>
                <th>D7</th>
                <th>D8</th>
                <th>D9</th>
                <th>D10</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for b_g_skill in b_g_skills %}
            <tr>
                <td>{{ b_g_skill.id }}</td>
                <td>{{ b_g_skill.name }}</td>
                <td>{{ b_g_skill.d1 }}</td>
                <td>{{ b_g_skill.d2 }}</td>
                <td>{{ b_g_skill.d3 }}</td>
                <td>{{ b_g_skill.d4 }}</td>
                <td>{{ b_g_skill.d5 }}</td>
                <td>{{ b_g_skill.d6 }}</td>
                <td>{{ b_g_skill.d7 }}</td>
                <td>{{ b_g_skill.d8 }}</td>
                <td>{{ b_g_skill.d9 }}</td>
                <td>{{ b_g_skill.d10 }}</td>
                <td>
                    <a href="{{ path('app_backgrounds_b_g_skill_show', {'id': b_g_skill.id}) }}">show</a>
                    <a href="{{ path('app_backgrounds_b_g_skill_edit', {'id': b_g_skill.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="13">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_backgrounds_b_g_skill_new') }}">Create new</a>
{% endblock %}
