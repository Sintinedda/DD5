{{ form_start(form) }}
    <div>
        {{ form_row(form.name) }}
        {{ form_row(form.slug) }}
        {{ form_row(form.source) }}
        {{ form_row(form.source_part) }}
    </div>
    <div>
        {{ form_row(form.competences) }}
        {{ form_row(form.languages) }}
        {{ form_row(form.equipment) }}
    </div>
    <div>
        {{ form_row(form.d1) }}
    </div>
    <div>
        {{ form_row(form.d2) }}
    </div>
    <div>
        {{ form_row(form.d3) }}
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
